@model IEnumerable<MVCIntroDemo.Models.Product.ProductViewModel>

<!DOCTYPE html>
<html>
<head>
    <title>All Products</title>
</head>
<body>
    <h1>All Products</h1>
    <ul>
        @foreach (var product in Model)
        {
            <li>@product.Name - $@product.Price</li>
        }
    </ul>
</body>
</html>